{
    "version": 1,
    "builds": [
        {
            "src": "node.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "node.js"
        }
    ]
}